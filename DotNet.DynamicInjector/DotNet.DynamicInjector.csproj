<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>tbertuzzi,andreluizsecco</Authors>
    <Company />
    <Copyright>Copyright Â©2020</Copyright>
    <RepositoryUrl>https://github.com/TBertuzzi/DotNetDynamicInjector</RepositoryUrl>
    <PackageProjectUrl>https://github.com/TBertuzzi/DotNetDynamicInjector</PackageProjectUrl>
    <PackageIcon>icone.png</PackageIcon>
    <PackageTags>net c# ioc dotnet injection netcore aspnet</PackageTags>
    <Description>Dynamic Injector for .Net Projects</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>DotNetDynamicInjector</PackageId>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1.0</Version>
    <PackageReleaseNotes>Added methods to add roles and new constructor</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.1" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Resources\icone.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
